<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="730.0" prefWidth="1290.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.tinaco.controllers.crearTinacoControllers">
   <children>
      <ImageView fitHeight="730.0" fitWidth="1290.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../accests/crearTinaco.jpg" />
         </image>
      </ImageView>
      <Button fx:id="botonAtrasCrearTinaco" layoutY="308.0" mnemonicParsing="false" onAction="#atrasCrearTinacoClick" opacity="0.0" prefHeight="108.0" prefWidth="266.0" text="Button" />
      <TextField fx:id="textNombreTinaco" layoutX="762.0" layoutY="334.0" prefHeight="55.0" prefWidth="384.0" promptText="Nombre del tinaco">
         <font>
            <Font size="25.0" />
         </font>
      </TextField>
      <TextField fx:id="textCapacidad" layoutX="762.0" layoutY="540.0" prefHeight="55.0" prefWidth="180.0" promptText="500L">
         <font>
            <Font size="25.0" />
         </font>
      </TextField>
      <ComboBox fx:id="comboSensor" layoutX="1008.0" layoutY="540.0" onAction="#comboSensorClick" prefHeight="55.0" prefWidth="155.0" promptText="Seleccione" />
      <Button fx:id="botonAgregarTinaco" layoutY="184.0" mnemonicParsing="false" onAction="#agregarTinacoClick" opacity="0.0" prefHeight="114.0" prefWidth="266.0" text="Button" />
   </children>
</AnchorPane>
